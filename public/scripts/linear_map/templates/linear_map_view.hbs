<div class="linear-map-visible-range"></div>
<div class="linear-map-position-marks">
  {{#each positionMarks}}
    <div class="linear-map-position-mark" data-base="{{base}}" style="top: {{top}}px;">
      {{label}}
    </div>
  {{/each}}
</div>
<div class="linear-map-features">
  {{#each features}}
    <div href="#" class="linear-map-feature linear-map-feature-{{type}}" data-feature_id="{{id}}">
      {{name}}
    </div>
  {{/each}} 
</div>
<div class="linear-map-enzymes">
  {{#each enzymes}}
    <div href="#" class="linear-map-enzyme" data-position="{{position}}">
      <span>{{label}}</span>
    </div>
  {{/each}} 
</div>