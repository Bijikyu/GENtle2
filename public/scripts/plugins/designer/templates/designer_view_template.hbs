<div class="designer-header">
  <div class="pull-right">
    <div style="margin-right: 30px; float: left; margin-top: 6px;">
      &nbsp;<input id="circularise-dna" type="checkbox" {{#if circulariseDna}}checked="1"{{/if}}>&nbsp;<label for="circularise-dna">Circularise</label>
    </div>
    <button class="btn btn-primary assemble-sequence-btn" {{#if submitDisabled}}disabled="disabled"{{/if}}>Assemble sequence</button>
  </div>
  <h4>{{sequenceName}}</h4>
</div>
<div class="designer-designed-sequence-outlet">
</div>
<div class="designer-available-sequences">
  <div class="designer-available-sequences-header">
    <h5>
      Available sequences
      <button class="btn btn-default btn-xs">Upload new sequences</button>
    </h5>
    <input type="file" novalidate class="file-upload-input" multiple style="display: none;"/>
  </div>
  <div class="designer-available-sequences-outlet outlet-1"></div>
  <div class="designer-available-sequences-outlet outlet-2"></div>
  {{!-- {{#each insertableSequences}}
    <div class="designer-available-sequence-outlet usable-sequence" data-sequence_id="{{.}}">
    </div>
  {{/each}} --}}

  {{!-- {{#if uninsertableSequences}}
    <br><br>
    <h5>
      &nbsp; {{uninsertableSequences}} unusable sequence(s)
      &nbsp;<a class="toggle-uninsertable-sequences">{{#if showUninsertableSequences}}hide{{else}}show{{/if}}</a>
    </h5>
  {{/if}} --}}
  {{!-- <div {{#unless showUninsertableSequences}}style="display: none;"{{/unless}}>
    {{#each incompatibleSequences}}
      <div class="designer-available-sequence-outlet unusable-sequence incompatible-sequences" data-sequence_id="{{.}}">
      </div>
    {{/each}}
    {{#each lackStickyEndSequences}}
      <div class="designer-available-sequence-outlet unusable-sequence lacks-sticky-end-sequences" data-sequence_id="{{.}}">
      </div>
    {{/each}}
  </div> --}}
  {{!-- </div> --}}

</div>
